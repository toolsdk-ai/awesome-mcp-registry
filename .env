# Server port
MCP_SERVER_PORT=3003

# Enable search service
ENABLE_SEARCH=true

# SANDBOX, Supported values: SANDOCK (recommended), DAYTONA, E2B, LOCAL
MCP_SANDBOX_PROVIDER=SANDOCK
# [Required] Visit https://sandock.ai to get Sandock API Key
SANDOCK_API_KEY=your-sandock-api-key-here

# Quick Start
# 1. Fill in your SANDOCK_API_KEY
# 2. Run: docker-compose up -d
# 3. Visit: http://localhost:3003
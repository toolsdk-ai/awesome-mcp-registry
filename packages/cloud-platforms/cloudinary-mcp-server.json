{
  "type": "mcp-server",
  "packageName": "cloudinary-mcp-server",
  "description": "Provides direct access to Cloudinary's Upload and Admin APIs for uploading, retrieving, searching, and managing digital media assets in your Cloudinary cloud.",
  "url": "https://github.com/yoavniran/cloudinary-mcp-server",
  "runtime": "node",
  "license": "unknown",
  "env": {
    "CLOUDINARY_CLOUD_NAME": {
      "description": "<cloud name>",
      "required": true
    },
    "CLOUDINARY_API_KEY": {
      "description": "<api-key>",
      "required": true
    },
    "CLOUDINARY_API_SECRET": {
      "description": "<api-secret>",
      "required": true
    }
  }
}
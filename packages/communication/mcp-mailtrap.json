{
	"type": "mcp-server",
	"packageName": "mcp-mailtrap",
	"description": "Enables sending transactional emails through the Mailtrap Email API.",
	"url": "https://github.com/railsware/mailtrap-mcp",
	"runtime": "node",
	"license": "unknown",
	"env": {
		"MAILTRAP_API_TOKEN": {
			"description": "your_mailtrap_api_token",
			"required": true
		},
		"DEFAULT_FROM_EMAIL": {
			"description": "your_sender@example.com",
			"required": true
		}
	},
	"name": "Mailtrap MCP"
}

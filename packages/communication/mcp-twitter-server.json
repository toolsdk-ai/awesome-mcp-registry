{
	"type": "mcp-server",
	"packageName": "mcp-twitter-server",
	"description": "Integrates with the X API v2 to enable post, search, and reply to tweets.",
	"url": "https://github.com/crazyrabbitltc/mcp-twitter-server",
	"runtime": "node",
	"license": "unknown",
	"env": {
		"X_API_KEY": {
			"description": "your_api_key",
			"required": true
		},
		"X_API_SECRET": {
			"description": "your_api_secret",
			"required": true
		},
		"X_ACCESS_TOKEN": {
			"description": "your_access_token",
			"required": true
		},
		"X_ACCESS_TOKEN_SECRET": {
			"description": "your_access_token_secret",
			"required": true
		}
	},
	"name": "MCP Twitter Server"
}

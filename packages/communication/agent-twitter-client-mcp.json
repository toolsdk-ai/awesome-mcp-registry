{
  "type": "mcp-server",
  "packageName": "agent-twitter-client-mcp",
  "description": "Provides a TypeScript-based server for interacting with Twitter, enabling tweet operations, user profile management, and Grok AI integration through multiple authentication methods.",
  "url": "https://github.com/ryanmac/agent-twitter-client-mcp",
  "runtime": "node",
  "license": "unknown",
  "env": {
    "AUTH_METHOD": {
      "description": "cookies",
      "required": true
    },
    "TWITTER_COOKIES": {
      "description": "[\"auth_token=YOUR_AUTH_TOKEN; Domain=.twitter.com\", \"ct0=YOUR_CT0_VALUE; Domain=.twitter.com\", \"twid=u%3DYOUR_USER_ID; Domain=.twitter.com\"]",
      "required": true
    }
  },
  "name": "Agent Twitter Client MCP",
  "stars": 12
}
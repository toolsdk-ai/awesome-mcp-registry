{
  "type": "mcp-server",
  "packageName": "mcp-server-bluesky",
  "description": "Integrates with Bluesky's social network API to enable profile management, posting, following, and engagement actions.",
  "url": "https://github.com/morinokami/mcp-server-bluesky",
  "runtime": "node",
  "license": "unknown",
  "env": {
    "BLUESKY_USERNAME": {
      "description": "username",
      "required": true
    },
    "BLUESKY_PASSWORD": {
      "description": "password",
      "required": true
    },
    "BLUESKY_PDS_URL": {
      "description": "https://bsky.social",
      "required": true
    }
  },
  "name": "Bluesky MCP Server"
}

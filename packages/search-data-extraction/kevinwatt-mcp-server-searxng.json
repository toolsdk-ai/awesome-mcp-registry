  "url": "https://github.com/kevinwatt/mcp-server-searxng",
  "runtime": "node",
  "license": "unknown",
  "env": {
    "SEARXNG_INSTANCES": {
      "description": "http://localhost:8080,https://searx.example.com",
      "required": true
    },
    "SEARXNG_USER_AGENT": {
      "description": "CustomBot/1.0",
      "required": true
    },
    "NODE_TLS_REJECT_UNAUTHORIZED": {
      "description": "0",
      "required": true
    }
  },
  "name": "SearXNG MCP Server",
  "stars": 18
}
{
  "type": "mcp-server",
  "packageName": "one-search-mcp",
  "description": "Provides a unified search and web scraping platform that integrates multiple search providers like SearxNG and Tavily, along with Firecrawl for advanced web content extraction, enabling flexible web data retrieval and structured information gathering.",
  "url": "https://github.com/yokingma/one-search-mcp",
  "runtime": "node",
  "license": "unknown",
  "env": {
    "SEARCH_PROVIDER": {
      "description": "searxng",
      "required": true
    },
    "SEARCH_API_URL": {
      "description": "http://127.0.0.1:8080",
      "required": true
    },
    "SEARCH_API_KEY": {
      "description": "YOUR_API_KEY",
      "required": true
    },
    "FIRECRAWL_API_URL": {
      "description": "http://127.0.0.1:3002",
      "required": true
    },
    "FIRECRAWL_API_KEY": {
      "description": "YOUR_API_KEY",
      "required": true
    }
  }
}
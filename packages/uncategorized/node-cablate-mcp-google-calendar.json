{
  "type": "mcp-server",
  "packageName": "@cablate/mcp-google-calendar",
  "description": "Integrates with Google Calendar API to enable creating, updating, and retrieving calendar events for seamless scheduling and event management.",
  "url": "https://github.com/cablate/mcp-google-calendar",
  "runtime": "node",
  "license": "unknown",
  "env": {
    "GOOGLE_CALENDAR_ID": {
      "description": "your_calendar_id",
      "required": true
    },
    "GOOGLE_TIME_ZONE": {
      "description": "your_time_zone",
      "required": true
    },
    "GOOGLE_CREDENTIALS_PATH": {
      "description": "your_credentials_path",
      "required": true
    }
  }
}
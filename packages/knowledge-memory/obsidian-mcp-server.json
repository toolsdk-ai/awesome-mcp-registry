{
  "type": "mcp-server",
  "packageName": "obsidian-mcp-server",
  "description": "Enables interaction with Obsidian vaults for file operations, content search, and metadata management, facilitating note-taking and knowledge base organization.",
  "url": "https://github.com/cyanheads/obsidian-mcp-server",
  "runtime": "node",
  "license": "unknown",
  "env": {
    "OBSIDIAN_API_KEY": {
      "description": "YOUR_API_KEY_FROM_OBSIDIAN_PLUGIN",
      "required": true
    },
    "OBSIDIAN_BASE_URL": {
      "description": "http://127.0.0.1:27123",
      "required": true
    },
    "OBSIDIAN_VERIFY_SSL": {
      "description": "false",
      "required": true
    },
    "OBSIDIAN_ENABLE_CACHE": {
      "description": "true",
      "required": true
    }
  },
  "name": "Obsidian MCP Server"
}
{
  "type": "mcp-server",
  "packageName": "keycloak-mcp",
  "description": "Integrates with Keycloak identity management to enable user creation, role assignment, group management, and client listing across different realms",
  "url": "https://github.com/haithamoumerzoug/keycloak-mcp",
  "runtime": "node",
  "license": "unknown",
  "env": {
    "KEYCLOAK_URL": {
      "description": "http://localhost:8080",
      "required": true
    },
    "KEYCLOAK_ADMIN": {
      "description": "admin",
      "required": true
    },
    "KEYCLOAK_ADMIN_PASSWORD": {
      "description": "admin",
      "required": true
    }
  },
  "name": "Keycloak MCP"
}

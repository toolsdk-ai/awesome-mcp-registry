{
	"type": "mcp-server",
	"packageName": "keycloak-model-context-protocol",
	"description": "Integrates with Keycloak Admin to provide streamlined user and realm management operations for identity and access control automation.",
	"url": "https://github.com/christophenglisch/keycloak-model-context-protocol",
	"runtime": "node",
	"license": "unknown",
	"env": {
		"KEYCLOAK_URL": {
			"description": "http://localhost:8080",
			"required": true
		},
		"KEYCLOAK_ADMIN": {
			"description": "admin",
			"required": true
		},
		"KEYCLOAK_ADMIN_PASSWORD": {
			"description": "admin",
			"required": true
		}
	},
	"name": "Keycloak Model Context Protocol"
}

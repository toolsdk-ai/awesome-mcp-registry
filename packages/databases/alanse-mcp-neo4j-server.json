{
  "type": "mcp-server",
  "packageName": "@alanse/mcp-neo4j-server",
  "description": "Integrates with Neo4j graph databases to enable natural language querying, node creation, and complex graph operations for knowledge exploration and data relationship modeling.",
  "url": "https://github.com/da-okazaki/mcp-neo4j-server",
  "runtime": "node",
  "license": "unknown",
  "env": {
    "NEO4J_URI": {
      "description": "bolt://localhost:7687",
      "required": true
    },
    "NEO4J_USERNAME": {
      "description": "neo4j",
      "required": true
    },
    "NEO4J_PASSWORD": {
      "description": "your-password",
      "required": true
    },
    "NEO4J_DATABASE": {
      "description": "neo4j",
      "required": true
    }
  },
  "name": "Neo4j MCP Server"
}

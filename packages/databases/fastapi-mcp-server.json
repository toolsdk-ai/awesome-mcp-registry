{
  "type": "mcp-server",
  "packageName": "fastapi-mcp-server",
  "description": "A sample FastAPI project that implements the Model Context Protocol (MCP), allowing AI assistants to connect to a PostgreSQL database and manage notes through natural language interactions.",
  "url": "https://github.com/texas000/mcp",
  "runtime": "python",
  "license": "unknown",
  "env": {
    "POSTGRES_URL": {
      "description": "PostgreSQL connection URL",
      "required": true
    },
    "POSTGRES_HOST": {
      "description": "PostgreSQL host address",
      "required": true
    },
    "POSTGRES_USER": {
      "description": "PostgreSQL username",
      "required": true
    },
    "POSTGRES_DATABASE": {
      "description": "PostgreSQL database name",
      "required": true
    },
    "POSTGRES_PASSWORD": {
      "description": "PostgreSQL password",
      "required": true
    },
    "POSTGRES_PRISMA_URL": {
      "description": "PostgreSQL connection URL for Prisma",
      "required": true
    },
    "POSTGRES_URL_NO_SSL": {
      "description": "PostgreSQL connection URL without SSL",
      "required": true
    },
    "POSTGRES_URL_NON_POOLING": {
      "description": "PostgreSQL connection URL for non-pooling connections",
      "required": true
    }
  },
  "name": "FastAPI MCP Server"
}
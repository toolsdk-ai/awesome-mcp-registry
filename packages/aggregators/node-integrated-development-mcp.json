  "url": "https://github.com/su-record/integrated-development-mcp",
  "runtime": "node",
  "license": "unknown",
  "env": {
    "GITHUB_TOKEN": {
      "description": "Your GitHub personal access token",
      "required": true
    },
    "SUPABASE_URL": {
      "description": "Your Supabase project URL",
      "required": true
    },
    "SUPABASE_ANON_KEY": {
      "description": "Your Supabase anonymous key",
      "required": true
    }
  }
}